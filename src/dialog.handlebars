<div class="dialog-container">
  <div class="dialog">
    <h2 class="dialog__title">{{title}}</h2>
    <div class="dialog__body">
      {{> @partial-block }}
    </div>
    <div class="dialog__footer">
        {{> Button label="Confirm" variant="primary" }}
        {{> Button label="Cancel" variant="error" }}
    </div>
  </div>
</div>
